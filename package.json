{
  "name": "vassist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:extension": "vite build --config vite.config.extension.js --watch",
    "dev:all": "concurrently \"vite\" \"vite build --config vite.config.extension.js --watch\"",
    "build": "vite build",
