{
  "name": "vassist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:extension": "vite build --config vite.config.extension.js --mode development --watch",
    "dev:android": "vite build --config vite.config.android.js --mode development && bunx cap sync android",
    "dev:all": "concurrently \"vite\" \"vite build --config vite.config.extension.js --mode development --watch\"",
    "build": "vite build --mode production",
    "build:extension": "vite build --config vite.config.extension.js --mode production",
    "build:extension:zip": "ZIP=true vite build --config vite.config.extension.js --mode production",
    "build:android": "vite build --config vite.config.android.js --mode production && bunx cap sync android",
    "build:all": "concurrently \"vite build --mode production\" \"vite build --config vite.config.extension.js --mode production\"",
    "cap:open:android": "bunx cap open android",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@babylonjs/core": "^8.30.0",
    "@babylonjs/havok": "^1.3.10",
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@heroicons/react": "^2.2.0",
    "@huggingface/transformers": "^3.7.6",
    "@tailwindcss/vite": "^4.1.14",
    "babylon-mmd": "1.0.0",
    "concurrently": "^9.2.1",
    "dexie": "^4.2.1",
    "jszip": "^3.10.1",
    "kokoro-js": "^1.2.1",
    "openai": "^6.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "archiver": "^7.0.1",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "esbuild": "^0.25.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
