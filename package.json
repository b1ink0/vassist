    "build:all": "concurrently \"vite build\" \"vite build --config vite.config.extension.js\"",
    "build:extension:prod": "NODE_ENV=production vite build --config vite.config.extension.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@babylonjs/core": "^8.30.0",
    "@babylonjs/havok": "^1.3.10",
    "@tailwindcss/vite": "^4.1.14",
    "babylon-mmd": "^0.68.0",
    "concurrently": "^9.2.1",
    "dexie": "^4.2.1",
    "openai": "^6.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
